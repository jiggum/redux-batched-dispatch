{
  "name": "redux-batched-dispatch",
  "version": "0.1.0",
  "description": "redux store enhancer to allow batched dispatch",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "/src"
  ],
  "config": {
    "prettier_target": "src/**/*.js"
  },
  "scripts": {
    "build": "yarn run build:clean && yarn run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "babel src --out-dir dist",
    "prettier:check": "yarn prettier --debug-check $npm_package_config_prettier_target",
    "prettier:write": "yarn prettier --write $npm_package_config_prettier_target",
    "lint:eslint": "eslint src",
    "lint": "yarn lint:eslint && yarn prettier:check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiggum/redux-batched-dispatch.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "keywords": [
    "redux",
    "react",
    "batch",
    "batched",
    "dispatch",
    "action",
    "performance"
  ],
  "author": "jiggum <0s.dongmin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiggum/redux-batched-dispatch/issues"
  },
  "homepage": "https://github.com/jiggum/redux-batched-dispatch",
  "dependencies": {
    "symbol-observable": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.2.0",
    "prettier": "1.17.0",
    "rimraf": "^2.6.3"
  },
  "peerDependencies": {
    "redux": ">=1.0.0"
  },
  "engines": {
    "node": ">=9.0.0",
    "yarn": ">=1.0.0"
  }
}
